<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#00FFFFFF"
    android:foregroundTint="#FFFFFF"
    tools:context=".MainActivity">


    <WebView
        android:id="@+id/jarvis"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#222222"
        android:focusableInTouchMode="true"
        android:foreground="@drawable/bac"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.suke.widget.SwitchButton
        android:id="@+id/camon"
        android:layout_width="63dp"
        android:layout_height="31dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.212"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />

    <me.ibrahimsn.lib.Speedometer
        android:id="@+id/speedometer"
        android:layout_width="330dp"
        android:layout_height="331dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="16dp"
        app:borderColor="#85353535"
        app:borderSize="5dp"
        app:fillColor="#C8C50000"
        app:layout_constraintStart_toEndOf="@+id/F"
        app:layout_constraintTop_toBottomOf="@+id/ipadd"
        app:maxSpeed="100"
        app:textColor="#f5f5f5"
        app:textGap="0.1dp" />

    <WebView
        android:id="@+id/cam"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="481dp"
        tools:layout_editor_absoluteY="54dp">

    </WebView>

    <ImageView
        android:id="@+id/hn"
        android:layout_width="63dp"
        android:layout_height="65dp"
        android:layout_marginTop="165dp"
        android:layout_marginEnd="132dp"
        android:clickable="true"
        android:rotation="90"
        android:soundEffectsEnabled="true"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?android:attr/fingerprintAuthDrawable"
        app:tint="#FBF6F6"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/dad2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="100dp"
        android:text="Line Sensing"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.061" />

    <com.suke.widget.SwitchButton
        android:id="@+id/switch_button2"
        android:layout_width="63dp"
        android:layout_height="31dp"
        android:visibility="gone" />

    <com.suke.widget.SwitchButton
        android:id="@+id/line"
        android:layout_width="63dp"
        android:layout_height="31dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="84dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/dad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lf"
        android:layout_width="126dp"
        android:layout_height="120dp"
        android:layout_centerInParent="true"
        android:layout_marginEnd="3dp"

        android:layout_marginBottom="26dp"
        android:padding="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toStartOf="@+id/rt"
        app:lottie_rawRes="@raw/wave" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/fr2"
        android:layout_width="126dp"
        android:layout_height="120dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="100dp"
        android:padding="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:lottie_rawRes="@raw/wave" />

    <ImageView
        android:id="@+id/left"
        android:layout_width="84dp"
        android:layout_height="94dp"
        android:clickable="true"
        android:rotation="90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/right"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.855"
        app:srcCompat="@drawable/fff" />

    <ImageView
        android:id="@+id/right"
        android:layout_width="84dp"
        android:layout_height="94dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        android:rotation="-90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.855"
        app:srcCompat="@drawable/fff" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/bc"
        android:layout_width="126dp"
        android:layout_height="120dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="232dp"
        android:padding="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:lottie_rawRes="@raw/wave" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/rt"
        android:layout_width="126dp"
        android:layout_height="120dp"
        android:layout_centerInParent="true"
        android:layout_marginEnd="4dp"

        android:padding="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.865"

        app:lottie_rawRes="@raw/wave" />

    <ImageView
        android:id="@+id/B"

        android:layout_width="87dp"
        android:layout_height="97dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="25dp"
        android:clickable="true"
        android:rotation="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fff" />

    <ImageView
        android:id="@+id/F"
        android:layout_width="84dp"
        android:layout_height="94dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="40dp"
        android:clickable="true"
        android:rotation="180"
        app:layout_constraintBottom_toTopOf="@+id/B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/fff" />

    <TextView
        android:id="@+id/ipadd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="IP Address!"
        android:textColor="#FAF6F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/ipadd2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Cam View"
        android:textColor="#FAF6F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <WebView
        android:id="@+id/web1"
        android:layout_width="32dp"
        android:layout_height="33dp"
        android:layout_marginEnd="364dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.51" />


    <TextView
        android:id="@+id/dad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="100dp"
        android:text=" Speed Control"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.366" />


    <ImageView
        android:id="@+id/wheel"
        android:layout_width="169dp"
        android:layout_height="180dp"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:contentDescription="@string/wheel"
        android:src="@drawable/wheel"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.906"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.914" />


    <SeekBar
        android:id="@+id/speed"
        style="@style/Widget.AppCompat.SeekBar"
        android:layout_width="137dp"
        android:layout_height="29dp"
        android:layout_marginTop="80dp"
        android:indeterminate="false"
        android:max="10"
        android:progress="5"
        android:progressDrawable="@drawable/custom_progress"
        android:rotation="-90"
        android:thumbTint="#D5070707"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toEndOf="@+id/dad"
        app:layout_constraintTop_toTopOf="parent" />

    <com.suke.widget.SwitchButton
        android:id="@+id/switch_button"
        android:layout_width="63dp"
        android:layout_height="31dp"
        android:layout_marginStart="44dp"
        app:layout_constraintBottom_toTopOf="@+id/F"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ipadd2"
        app:layout_constraintVertical_bias="0.382" />

    <com.ramijemli.percentagechartview.PercentageChartView
        android:id="@+id/battey"
        android:layout_width="95dp"
        android:layout_height="88dp"
        android:contentDescription="Battery"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.682"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.121"
        app:pcv_animDuration="800"
        app:pcv_animInterpolator="accelerate"
        app:pcv_backgroundBarColor="#7741ECB1"
        app:pcv_backgroundColor="#1B676262"
        app:pcv_drawBackgroundBar="false"
        app:pcv_gradientColors="#00BCD4, #2196F3 , #F44336 "
        app:pcv_gradientDistributions="0.2 , 0.5 , 0.8"
        app:pcv_gradientType="radial"
        app:pcv_mode="fill"
        app:pcv_orientation="clockwise"
        app:pcv_progress="50"
        app:pcv_progressColor="#F39A0202"
        app:pcv_startAngle="90"
        app:pcv_textSize="15dp"
        app:pcv_typeface="interstellar.ttf" />


</androidx.constraintlayout.widget.ConstraintLayout>